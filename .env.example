# API Configuration
API_URL=https://resto-api-servers-beta.onrender.com/api
API_TOKEN=your_api_token_here
RESTAURANT_ID=your_restaurant_id_here

# WebSocket Configuration
WS_ENDPOINT=wss://resto-api-servers-beta.onrender.com

# Polling Configuration (fallback if WebSocket fails)
POLL_INTERVAL=5000

# Logging
LOG_LEVEL=info

# Browser Integration
ENABLE_BROWSER_INTEGRATION=true
BROWSER_PORT=3001

# Auto Start Configuration
AUTO_START=true
MINIMIZE_TO_TRAY=true

# HTTP Server Port
PORT=3000 